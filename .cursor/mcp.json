{
  "mcpServers": {
    "task-master-ai": {
      "command": "npx",
      "args": ["-y", "task-master-ai"],
      "env": {
        "TASK_MASTER_TOOLS": "standard",
        "OLLAMA_BASE_URL": "http://localhost:11434/api",
        "OLLAMA_API_KEY": "",
        "ANTHROPIC_API_KEY": "",
        "PERPLEXITY_API_KEY": "",
        "OPENAI_API_KEY": "",
        "GOOGLE_API_KEY": "",
        "XAI_API_KEY": "",
        "OPENROUTER_API_KEY": "",
        "MISTRAL_API_KEY": "",
        "AZURE_OPENAI_API_KEY": "",
        "GROQ_API_KEY": "",
        "GITHUB_API_KEY": "",
        "LOG_LEVEL": "info",
        "DEBUG": "false"
      },
      "disabled": false,
      "timeout": 30000,
      "retryLimit": 3
    }
  },
  "global": {
    "enableAnalytics": true,
    "enableErrorReporting": false,
    "logLevel": "info"
  }
}